{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "Node Boilerplate (PostgreSQL, Typescript, Node.js, Express.js)",
  "scripts": {
    "dev": "NODE_ENV=local nodemon",
    "prod": "NODE_ENV=production node -r ts-node/register -r tsconfig-paths/register ./build/src/index.js",
    "build": "NODE_ENV=production tsc -p .",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:create": "NODE_ENV=local yarn typeorm migration:create -n",
    "migration:generate": "NODE_ENV=local yarn typeorm migration:generate -n",
    "setup:local": "NODE_ENV=local yarn typeorm migration:run",
    "setup:prod": "NODE_ENV=production yarn typeorm migration:run",
    "setup:test": "NODE_ENV=test yarn typeorm migration:run",
    "deploy": "sh deployment/prod-deploy-aws.sh",
    "lint": "eslint . --ext .ts",
    "test": "NODE_ENV=test jest -i",
    "test:coverage": "NODE_ENV=test jest --coverage"
  },
  "dependencies": {
    "@hapi/joi": "17.1.0",
    "@types/node": "^14.0.14",
    "bcrypt": "^5.0.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1",
    "express-joi-validation": "4.0.3",
    "express-promise-router": "3.0.3",
    "helmet": "3.21.2",
    "http-status": "1.4.2",
    "logdna-winston": "^2.3.2",
    "logform": "2.1.2",
    "morgan": "1.9.1",
    "nodemon": "2.0.2",
    "normalize-email": "^1.1.1",
    "passport": "^0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.2.1",
    "postgres": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.9.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.6",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/compression": "1.7.0",
    "@types/cors": "2.8.6",
    "@types/dotenv-safe": "8.1.0",
    "@types/express": "4.17.2",
    "@types/express-promise-router": "3.0.0",
    "@types/faker": "4.1.10",
    "@types/hapi__joi": "16.0.12",
    "@types/helmet": "0.0.45",
    "@types/http-status": "1.1.2",
    "@types/jest": "25.1.3",
    "@types/logform": "1.10.1",
    "@types/morgan": "1.7.37",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "2.0.8",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "eslint": "6.8.0",
    "faker": "4.1.0",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1"
  }
}
